          ['2018',  <%= (@lumensAnnualReturns * 10) %>, <%= @lumensPrice + (@savingsAnnualReturns * 10) %>],
          ['2019',  <%= (@lumensAnnualReturns * 11) %>, <%= @lumensPrice + (@savingsAnnualReturns * 11) %>],
          ['2020',  <%= (@lumensAnnualReturns * 12) %>, <%= @lumensPrice + (@savingsAnnualReturns * 12) %>],
          ['2021',  <%= (@lumensAnnualReturns * 13) %>, <%= @lumensPrice + (@savingsAnnualReturns * 13) %>],
          ['2022',  <%= (@lumensAnnualReturns * 14) %>, <%= @lumensPrice + (@savingsAnnualReturns * 14) %>],
          ['2023',  <%= (@lumensAnnualReturns * 15) %>, <%= @lumensPrice + (@savingsAnnualReturns * 15) %>],
          ['2024',  <%= (@lumensAnnualReturns * 16) %>, <%= @lumensPrice + (@savingsAnnualReturns * 16) %>],
      ['2025',  <%= (@lumensAnnualReturns * 17) %>, <%= @lumensPrice + (@savingsAnnualReturns * 17) %>],
      ['2026',  <%= (@lumensAnnualReturns * 18) %>, <%= @lumensPrice + (@savingsAnnualReturns * 18) %>],
      ['2027',  <%= (@lumensAnnualReturns * 19) %>, <%= @lumensPrice + (@savingsAnnualReturns * 19) %>],
      ['2028',  <%= (@lumensAnnualReturns * 20) %>, <%= @lumensPrice + (@savingsAnnualReturns * 20) %>],
      ['2029',  <%= (@lumensAnnualReturns * 21) %>, <%= @lumensPrice + (@savingsAnnualReturns * 21) %>],
      ['2030',  <%= (@lumensAnnualReturns * 22) %>, <%= @lumensPrice + (@savingsAnnualReturns * 22) %>],
      ['2031',  <%= (@lumensAnnualReturns * 23) %>, <%= @lumensPrice + (@savingsAnnualReturns * 23) %>],
      ['2032',  <%= (@lumensAnnualReturns * 24) %>, <%= @lumensPrice + (@savingsAnnualReturns * 24) %>],
      ['2033',  <%= (@lumensAnnualReturns * 25) %>, <%= @lumensPrice + (@savingsAnnualReturns * 25) %>]
        ]);

        var options = {
          chartArea:{left:60, top:20, bottom:10, width:'65%', height:'80%'},
      vAxis: {format: '$#,###'},
      
        };

        var chart = new google.visualization.LineChart(document.getElementById('chart_div'));
        chart.draw(data, options);
