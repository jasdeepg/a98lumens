<section id="content" class="clearfix">
  <div class="container"> 
  
    <!--divider here-->
    <div class="spacer-20px"></div>
    
    <h1>Your personal <span class="color">98lumens</span> Solar proposal</h1>

 
        <!--pricing intro starts-->
    <div class="span12 pricing-intro">
      <h1>98lumens <span class="color">Pricing</span></h1>
    </div>
    <!--pricing intro ends--> 
    
    <!--pricing 4 columns starts-->
    <div class="row">
      <div class="span12">
        <div class="clearfix pricing three-col"> 
        
          <!--pricing second column / focused starts-->
          <div class="price-column focused">
            <div class="table-th">
              <p class="table-slug"><%= @setPanels %> Solar Panels</p>
              <h4>98lumens </h4>
              <sup>$</sup><%= @lumensPrice %><sup>.00</sup></div>
            <div class="clearfix"></div>
            <div class="table-content">
              <p class="sign-available">Price per solar panel: <span class="color">$<%= @lumensPanelPrice %></span></p>
              <p class="sign-available">Effective interest rate: <span class="color"><%= @lumensinterest %>%</span></p>
              <p>25 Year returns: <span class="color">$<%= @lumensLifeReturns %></span></p>
              <p class="sign-available">Roofless solar</p>
              <p class="sign-available">Liquidity/Mobility</p>
              <p class="sign-available">Worry free</p>
              <p class="sign-available">CO2 emissions saved: <span class="color"><%= number_to_human(@cO2Offset, :precision=>0) %> Metric Tons </span></p>
              <p class="sign-available">Equivalent trees planted: <span class="color"><%= number_to_human(@plantedTrees, :precision=>0) %> Trees </span></p>
              <div class="clearfix"></div>
            </div>
          </div>
          <!--pricing second column ends-->         
          
          <!--pricing first column starts-->
          <div class="price-column first">
            <div class="table-th">
              <h4>Rooftop Solar </h4>
              <sup>$</sup><%= @homePrice %><sup>.00</sup> </div>
            <div class="clearfix"></div>
            <div class="table-content">
              <p>Price per solar panel: <span class="color">$<%= @homePanelPrice %></span></p>
              <p>Effective interest rate: <span class="color"><%= @homeinterest %>%</span></p>
              <p>25 Year returns: <span class="color">$<%= @homeLifeReturns %></span></p>
              <p class="sign-notavailable">Roofless solar</p>
              <p class="sign-notavailable">Liquidity/Mobility</p>
              <p class="sign-notavailable">Worry free</p>
              <p>CO2 emissions saved: <span class="color"><%= number_to_human(@cO2Offset * 2/3, :precision=>0) %> Metric Tons </span></p>
              <p>Equivalent trees planted: <span class="color"><%= number_to_human(@plantedTrees * 2/3, :precision=>0) %> Trees </span></p>
              <div class="clearfix"></div>
            </div>
          </div>
          <!--pricing first column ends--> 
          

          
          <!--pricing third column / last starts-->
          <div class="price-column last">
            <div class="table-th">
              <h4>Savings Account </h4>
              <sup>$</sup><%= @lumensPrice %><sup>.00</sup></div>
            <div class="clearfix"></div>
            <div class="table-content">
              <p>Price per solar panel: <span class="color">NA</span></p>
              <p>Effective interest rate: <span class="color"><%= @Savingsinterest %>%</span></p>
              <p>25 Year returns: <span class="color">$<%= @savingsLifeReturns %></span></p>
              <p>NA</p>
              <p>Liquidity/Mobility</p>
              <p>Worry free</p>
              <p>CO2 emissions saved: <span class="color"> 0 Metric Tons </span></p>
              <p>Equivalent trees planted: <span class="color"> 0 Trees </span></p>
              <div class="clearfix"></div>   
            </div>
          </div>
          <!--pricing third column ends--> 
          
        </div>
      </div>
    </div>
    <!--pricing 4 columns ends-->  
    
        
    <!--spacer here-->
    <div class="spacer-30px"></div>
    
    
    <h1><span class="color">Economics</span></h1>
  
    
    <!--features starts-->
    <div class="row">
      <div class="span12">
        <!--feature list starts-->
        <ul class="features-big">
          
          <!--feature one starts-->
          <li>
            <div class="row">
            
            <div class="span6 relativeto"> <%= image_tag("preview/img-finance98.jpg") %>
