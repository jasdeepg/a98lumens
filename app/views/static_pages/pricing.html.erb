                <div class="overlay-inner">
                    <ul class="list-unstyled">

                        <li><span class="color">98lumens</span> - <%= @setPanels %> Panels</li>
                        <li>Annual Electricity production: <h3><%= number_to_human(@annualProduction, :precision=>0) %> kWh</h3></li>
                  <li>Lifetime production:  <h3><%= @lifetimeProduction %> kWh</h3></li>
                        <li>Annual returns to you: <h3>$<%= @lumensAnnualReturns %></h3></li>
                        <li>Total 25 year lifetime returns: <h3>$<%= @lumensLifeReturns %></h3></li>
                        <li>Effective interest rate: <h3><%= @lumensinterest%>%</h3></li>

                    </ul>
                </div>
               </div>
            </div>
            
            <div class="span6">
              
               <h5>Your cumulative returns</h5>
              
               <div id="chart_div" style="width: 500px; height: 400px;"></div>
                <!-- <img src="img/preview/img-window.png" alt="image" > </div> -->
            </div>
           </div>
          </li>
          <!--feature one ends--> 
          
     
        </ul>
        <!--feature list ends--> 
        
      </div>
    </div>
    <!--features ends-->     
  
    
 
    
    <!--spacer here-->
    <div class="spacer-30px"></div>
    
    <!--box starts-->
    <div class="row">
      <div class="span12">
        <div class="dark-box">
          <h1>Reserve your panels for <span class="color">Free!</span></h1>
           <ul class="list-inline">
            <li>Limited time offer. Reserve today!</li>
          </ul>
          <a href="reserve.html?SetPanels=<%= @setPanels %>&UserPanels=<%= @userPanels%>" class="mybtn"><span class="color">Reserve Now!</span></a> </div>
      </div>
    </div>
    <!--box ends--> 
    
  </div>
